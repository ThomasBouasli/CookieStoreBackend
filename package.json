{
  "name": "cookie-store",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ThomasBouasli/CookieStore.git",
  "author": "Thomas Bouasli <thomas.bouasli@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"dotenv -e .env.dev -- tsnd -r tsconfig-paths/register src/Application/index.ts\" \"yarn --cwd ./src/Adapter/view dev\"",
    "start": "dotenv -e .env -- node -r ts-node/register/transpile-only -r tsconfig-paths/register build/Application/index.js",
    "test": "dotenv -e .env.test -- jest",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "build": "tsc && rm -rf build/Adapter/view && mkdir build/Adapter/view && cd src/Adapter/view && yarn build"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.40",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "concurrently": "^7.2.2",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "prisma": "^3.15.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2"
  },
  "_moduleAliases": {
    "@Entity/*": "build/Entity/*/index.js",
    "@Adapter/*": "build/Adapter/*",
    "@Application": "build/Application/index.js",
    "@Application/*": "build/Application/*",
    "@Util/*": "build/Util/*",
    "@Controller/*": "build/Feature/*/Controller/*",
    "@UseCase/*": "build/Feature/*/UseCase/*"
  }
}
